{
  "extends": "recommended",
  "rules": {
    "CompileStatic": {
      "enabled": false
    },
    "ExplicitCallToEqualsMethod": {
      "enabled": false
    },
    "Indentation": {
      "spacesPerIndentLevel": 2
    },
    "LineLength": {
      "length": 240
    },
    "NestedBlockDepth": {
      "maxNestedBlockDepth": 14
    }
  }
}
